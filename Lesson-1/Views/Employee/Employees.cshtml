@model IEnumerable<WebStore.Models.Employee>
<div class="container">
    <table class="table">
        <thead>
        <tr>
            <th>Id</th>
            <th>Фамилия</th>
            <th>Имя</th>
        </tr>
        </thead>
        <tbody>
        @foreach (var employee in Model)
        {
            <tr>
                <td>
                    @Html.ActionLink(@employee.Id.ToString(), "EmployeeCard", new { id = employee.Id})
                </td>
                <td>@employee.SecondName</td>
                <td>@employee.FirstName</td>
            </tr>
        }
        </tbody>
    </table>
    <a href="/">На главную</a>
</div>