@model IEnumerable<WebStore.Models.Employee>
<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Employees</title>
</head>
<body>
<table>
    <thead>
    <tr>
        <th>Id</th>
        <th>Фамилия</th>
        <th>Имя</th>
    </tr>
    </thead>
    <tbody>
    @foreach (var employee in Model)
    {
        <tr>
            <td>
                @Html.ActionLink(@employee.Id.ToString(), "EmployeeCard", new { id = employee.Id})
            </td>
            <td>@employee.SecondName</td>
            <td>@employee.FirstName</td>
        </tr>
    }
    </tbody>
</table>
<a href="/">На главную</a>
</body>
</html>
