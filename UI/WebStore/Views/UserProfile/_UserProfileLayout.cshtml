@{
    Layout = "_Layout";
}
<div class="container">
    <div class="row">
        <div class="col-md-3">
            <aside>
                <partial name="Partial/_UserProfileMenu"/>
            </aside>
        </div>
        <div class="col-md-9">
            @RenderBody()
        </div>
    </div>
</div>

@section Scripts
{
    @await RenderSectionAsync("Scripts", false)
}
@section Styles
{
    @await RenderSectionAsync("Styles", false)
}